{
    "[json*]": { "editor.defaultFormatter": "vscode.json-language-features" },
    "cursor.composer.shouldShowMarkdownHoverParticipantActions2": true,
    "cursor.general.disableHttp2": true,
    "cursor.general.enableShadowWorkspace": true,
    "cursor.terminal.usePreviewBox": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorSurroundingLines": 8,
    "editor.defaultFormatter": "Shopify.ruby-extensions-pack",
    "editor.fontFamily": "'Monaspace Krypton', monospace",
    "editor.fontLigatures": "'calt', 'ss01', 'ss02', 'ss03', 'ss04', 'ss06', 'ss09', 'liga', 'cv30', 'cv60'",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.guides.bracketPairs": "active",
    "editor.guides.highlightActiveIndentation": "always",
    "editor.minimap.autohide": true,
    "editor.minimap.enabled": true,
    "editor.rulers": [100],
    "editor.scrollBeyondLastLine": false,
    "editor.smoothScrolling": true,
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "editor.wrappingIndent": "deepIndent",
    "editor.wrappingStrategy": "advanced",
    "explorer.autoReveal": true,
    "extensions.ignoreRecommendations": true,
    "files.autoSave": "onFocusChange",
    "git.openRepositoryInParentFolders": "never",
    "githubPullRequests.pullBranch": "never",
    "gitlens.codeLens.authors.enabled": true,
    "gitlens.currentLine.fontFamily": "Monaspace Neon",
    "gitlens.currentLine.format": "${author, }${agoOrDate}${' via  'pullRequest}${ â€¢ id}",
    "gitlens.currentLine.scrollable": false,
    "gitlens.currentLine.uncommittedChangesFormat": "${author, }${agoOrDate}${' via  'pullRequest}",
    "gitlens.graph.dimMergeCommits": true,
    "gitlens.plusFeatures.enabled": false,
    "keyboard.touchbar.enabled": false,
    "references.preferredLocation": "view",
    "rubyLsp.formatter": "rubocop_internal",
    "rubyLsp.rubyVersionManager": { "miseExecutablePath": "/opt/homebrew/bin/mise", "identifier": "mise" },
    "search.searchEditor.focusResultsOnSearch": true,
    "search.sortOrder": "modified",
    "security.promptForLocalFileProtocolHandling": false,
    "security.workspace.trust.banner": "always",
    "sorbet.enabled": true,
    "terminal.external.osxExec": "WezTerm.app",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.enableVisualBell": true,
    "terminal.integrated.fontLigatures": true,
    "terminal.integrated.fontWeight": 200,
    "terminal.integrated.fontWeightBold": 400,
    "terminal.integrated.smoothScrolling": true,
    "terminal.integrated.suggest.enableExtensionCompletions": true,
    "terminal.integrated.suggest.enabled": true,
    "window.autoDetectColorScheme": true,
    "window.density.editorTabHeight": "compact",
    "workbench.colorCustomizations": { "[*Dark*]": { "editorRuler.foreground": "#444" }, "[*Light*]": { "editorRuler.foreground": "#EEE" } },
    "workbench.colorTheme": "GitHub Dark Dimmed",
    "workbench.editor.editorActionsLocation": "titleBar",
    "workbench.editor.preferHistoryBasedLanguageDetection": true,
    "workbench.editor.revealIfOpen": true,
    "workbench.editor.tabActionLocation": "left",
    "workbench.editor.wrapTabs": true,
    "workbench.list.smoothScrolling": true,
    "workbench.preferredDarkColorTheme": "GitHub Dark Dimmed",
    "workbench.preferredHighContrastColorTheme": "GitHub Dark Dimmed",
    "workbench.preferredHighContrastLightColorTheme": "GitHub Light High Contrast",
    "workbench.preferredLightColorTheme": "GitHub Light High Contrast",
    "workbench.tree.indent": 6,
    "workbench.tree.renderIndentGuides": "always",
    // "rubyTestExplorer.filePattern": [
    //     "/packs/carrier_registry/**/spec/**/*_spec.rb",
    //     "/spec/**/*_spec.rb"
    // ],
    // "rubyTestExplorer.logpanel": true,
    // "rubyTestExplorer.rspecCommand": "mise exec -- bundle exec rspec",
    // "rubyTestExplorer.rspecDirectory": ".",
    // "rubyTestExplorer.testFramework": "rspec",
}
